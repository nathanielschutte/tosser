{
    "users": {
        "Source": [
            "users",
            "users/*/height",
            "users/*/contact/*",
            "users/*/extra/**"
        ],
        "Columns": {
            "@include": "users.table.col.json"
        }
    },
    "users_friends": {
        "Source": [
            "users/*/friends"
        ],
        "Columns": {
            "@ignore": "*",
            "@keep": "first_name" 
        }
    },
    "user_aliases": {
        "Source": [
            "users/aliases"
        ],
        "Columns": {
            "@value": "alias"
        }
    }
}
